function setscheme
    set -Ux fish_color_normal normal
    set -Ux fish_color_command cyan 
    set -Ux fish_color_keyword magenta
    set -Ux fish_color_quote yellow
    set -Ux fish_color_redirection normal 
    set -Ux fish_color_end yellow
    set -Ux fish_color_error red
    set -Ux fish_color_param magenta
    set -Ux fish_color_comment brblack
    set -Ux fish_color_operator green
    set -Ux fish_color_escape magenta
    set -Ux fish_color_autosuggestion brblack
    set -Ux fish_color_user green
    set -Ux fish_color_host green
    set -Ux fish_color_host_remote red
    set -Ux fish_color_cancel red
    set -Ux fish_color_cwd green
    
    # Completion Pager Colors
    set -Ux fish_pager_color_progress brblack
    set -Ux fish_pager_color_prefix cyan
    set -Ux fish_pager_color_completion normal
    set -Ux fish_pager_color_description brblack
end
